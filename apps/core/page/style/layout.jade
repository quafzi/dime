doctype 5
html(ng-app='dime')
  head
    title Dime Time Tracking
    //[if lt IE 9]
      script(src="/scripts/html5shiv.js")
    each style in styles
      link(href="#{config.style.urlPrefix}#{style}", rel='stylesheet', type='text/css')
  body
    .container
      block content
  each script in scripts
    script(src="#{config.script.urlPrefix}#{script}")
  each template, name in templates
    script(type="text/ng-template", id="#{name}")!= template
